<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="controller.MenusController">
    <menus>
        <Menu text="Estudante">
            <items>
                <MenuItem fx:id="menuEstudanteInclui" onAction="#incluiOnAction" text="Incluir" />
                <MenuItem fx:id="menuEstudanteAltera" onAction="#alteraOnAction" text="Alterar" />
                <MenuItem fx:id="menuEstudanteExclui" mnemonicParsing="false" onAction="#exluiOnAction" text="Excluir" />
                <MenuItem fx:id="menuEstudanteSair" mnemonicParsing="false" onAction="#sairOnAction" text="Sair" />
            </items>
        </Menu>
        <Menu text="Professor">
            <items>
                <MenuItem fx:id="menuTelaVazia" onAction="#telaVaziaOnAction" text="Tela Vazia" />
                <MenuItem fx:id="menuProfessorSair" mnemonicParsing="false" onAction="#sairOnAction" text="Sair" />
            </items>
        </Menu>
    </menus>
</MenuBar>
